<table id="evm-indicator-option_value-table" border="1" bordercolor = "#bbb">
  <tr>
    <th>#</th>
    <th><%=l(:field_subject)%></th>
    <th>BAC</th>
    <th>Complete EV</th>
    <th>PV</th>
    <th>EV</th>
    <th>AC</th>
    <th>SV</th>
    <th>CV</th>
    <th>SPI</th>
    <th>CPI</th>
  </tr>
  <% parent_issue_evm.each do |parent_issue_id, evm| %>
    <tr>
      <td><%=parent_issue_link parent_issue_id%></td>
      <td><%=parent_issue(parent_issue_id).subject%></td>
      <td><%=evm.bac%></td>
      <td><%=evm.complete_ev%> %</td>
      <td><%=evm.today_pv%></td>
      <td><%=evm.today_ev%></td>
      <td><%=evm.today_ac%></td>
      <td><%=evm.today_sv%></td>
      <td><%=evm.today_cv%></td>
      <td><%=spi_color%><%=evm.today_spi%></td>
      <td><%=cpi_color%><%=evm.today_cpi%></td>
    </tr>
  <% end %>
</table>
<br>